{
    "name": "todo-backend",
    "version": "1.0.0",
    "main": "src/index.js",
    "license": "MIT",
    "type": "module",
    "scripts": {
        "start": "nodemon src/index.js"
    },
    "dependencies": {
        "@prisma/client": "^4.15.0",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "prisma": "^4.15.0"
    },
    "devDependencies": {
        "@types/node": "^20.3.1",
        "body-parser": "^1.20.2",
        "cors": "^2.8.5",
        "eslint": "^8.43.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-plugin-import": "^2.27.5",
        "nodemon": "^2.0.22",
        "ts-node": "^10.9.1",
        "typescript": "^5.1.3"
    },
    "eslintConfig": {
        "eslintConfig": {
            "extends": "airbnb-base",
            "env": {
                "es6": true,
                "browser": true
            },
            "rules": {
                "brace-style": [
                    "error",
                    "stroustrup"
                ],
                "comma-dangle": [
                    "error",
                    "never"
                ],
                "no-unused-vars": [
                    "warn"
                ],
                "no-var": [
                    "off"
                ],
                "one-var": [
                    "off"
                ]
            }
        }
    }
}
